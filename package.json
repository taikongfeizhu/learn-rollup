{
  "name": "learn-rollup",
  "version": "0.1.0",
  "description": "This is an example project to accompany a tutorial on using Rollup.",
  "main": "dist/js/demo.js",
  "module": "dist/js/demo.moudle.js",
  "browser": "dist/js/demo.umd.js",
  "style": "dist/css/style.css",
  "scripts": {
    "build": "rm -rf ./dist/js && rm -rf ./dist/css && NODE_ENV=production rollup -c",
    "dev": "NODE_ENV=development rollup -c --watch",
    "reload": "livereload 'build/'",
    "start": "npm-run-all --parallel reload dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jlengstorf/learn-rollup.git"
  },
  "author": "Jason Lengstorf <jason@lengstorf.com> (@jlengstorf)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/taikongfeizhu/learn-rollup/issues"
  },
  "homepage": "https://github.com/taikongfeizhu/learn-rollup/blob/master/README.md",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "autoprefixer": "^9.7.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.7.2",
    "livereload": "^0.8.2",
    "npm-run-all": "^4.1.5",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-simple-vars": "^5.0.2",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-watch": "^4.3.1"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
